    {
      "type"    : "package",
      "package" : {
          "name"    : "enyo/dropzone",
          "version" : "dev-master",
          "type"    : "drupal-library",
          "source"  : {
              "url"       : "https://github.com/enyo/dropzone.git",
              "type"      : "git",
              "reference" : "origin/master"
          }
       }
    },
    {
      "type"    : "package",
      "package" : {
          "name"    : "kenwheeler/slick",
          "version" : "dev-master",
          "type"    : "drupal-library",
          "source"  : {
              "url"       : "https://github.com/kenwheeler/slick.git",
              "type"      : "git",
              "reference" : "origin/master"
          }
       }
    },
    {
      "type"    : "package",
      "package" : {
          "name"    : "vardot/ace",
          "version" : "dev-master",
          "type"    : "drupal-library",
          "source"  : {
              "url"       : "https://github.com/vardot/ace.git",
              "type"      : "git",
              "reference" : "origin/master"
          }
       }
    },
    {
      "type"    : "package",
      "package" : {
          "name"    : "dinbror/blazy",
          "version" : "dev-master",
          "type"    : "drupal-library",
          "source"  : {
              "url"       : "https://github.com/dinbror/blazy.git",
              "type"      : "git",
              "reference" : "origin/master"
          }
       }
    }
  ],
